<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Home">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="417dp"
        android:layout_height="219dp"
        android:background="@color/white"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="112dp"
            android:layout_height="94dp"

            app:srcCompat="@drawable/finallogo" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginTop="15dp"
            android:text="Welcome Mr/Ms:-"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="20dp" />

        <EditText
            android:id="@+id/Username"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:ems="10"
            android:inputType="textPersonName" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="291dp"
        android:layout_height="94dp"
        android:layout_marginStart="105dp"
        android:fontFamily="cursive"
        android:text="Jasmine Mobile"
        android:textColor="#AC3434"
        android:textAlignment="center"
        android:textSize="50dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="409dp"
        android:layout_height="139dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/samsung"

        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="21dp"
            android:layout_marginEnd="177dp"
            android:fontFamily="sans-serif-black"

            android:padding="25dp"
            android:text="Samsung"
            android:textAlignment="center"
            android:textSize="30sp">

        </TextView>

        <Button
            android:id="@+id/buttonSamsung"
            android:layout_width="69dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="69dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="53dp"
            android:background="@drawable/custome_button"
            android:text=">"
            android:textSize="30dp"
            app:iconTint="#FFFFFF" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="410dp"
        android:layout_height="144dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/realme"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="21dp"
            android:layout_marginEnd="177dp"
            android:fontFamily="sans-serif-black"
            android:padding="25dp"
            android:text="RealMe"
            android:textAlignment="center"
            android:textSize="30sp">

        </TextView>

        <Button
            android:id="@+id/buttonRealMe"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="69dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="52dp"
            android:background="@drawable/custome_button"
            android:text=">"
            android:textSize="30dp"
            app:iconTint="#FFFFFF" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="406dp"
        android:layout_height="147dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/iphone"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout2">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="21dp"
            android:layout_marginEnd="177dp"
            android:fontFamily="sans-serif-black"
            android:padding="25dp"
            android:text="Iphone"
            android:textAlignment="center"
            android:textSize="30sp">

        </TextView>

        <Button
            android:id="@+id/buttoniphone"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="69dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="52dp"
            android:background="@drawable/custome_button"
            android:text=">"
            android:textSize="30dp"
            app:iconTint="#FFFFFF" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>